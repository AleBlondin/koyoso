---

dev_env: true
# List of databases to be created
postgresql_databases:
  - name: koyoso
    hstore: yes
    uuid_ossp: yes

# List of users to be created
postgresql_users:
  - name:  koyoso
    pass: koyoso

postgresql_user_privileges:
  - name: koyoso
    db: koyoso
    priv: "ALL"
    role_attr_flags: "SUPERUSER"
